function ValidateInputData($type = null, $name = null)
{
    $filterTypeArray = array(
        "Post" 	=> 	INPUT_POST,
        "Get"		=>	INPUT_GET,
        "Cookie" 	=>	INPUT_COOKIE,
        "Env" 		=> 	INPUT_ENV,
        "Server" 	=> 	INPUT_SERVER,
    );
 if(filter_has_var($filterTypeArray[$type], $name))
    {
        $inputdata = filter_input($filterTypeArray[$type], $name);
    }
    return $inputdata;
}

